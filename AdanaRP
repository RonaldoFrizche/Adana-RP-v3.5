        local espBtn = Instance.new("TextButton", espFrame)
        espBtn.Position = UDim2.new(0.75, 0, 0.1, 0)
        espBtn.Size = UDim2.new(0.2, 0, 0.8, 0)
        espBtn.BackgroundColor3 = Color3.fromRGB(100, 0, 255)
        espBtn.Text = "Aç/Kapat"
        espBtn.TextColor3 = Color3.new(1, 1, 1)
        espBtn.TextScaled = true
        Instance.new("UICorner", espBtn).CornerRadius = UDim.new(0, 5)
        espBtn.MouseButton1Click:Connect(function()
            isEspOn = not isEspOn
            if isEspOn then
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer then
                        task.spawn(function()
                            createESP(player)
                        end)
                    end
                end
                espLabel.Text = "ESP: AÇIK"
                espBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
            else
                for _, player in pairs(Players:GetPlayers()) do
                    removeESP(player)
                end
                espLabel.Text = "ESP: KAPALI"
                espBtn.BackgroundColor3 = Color3.fromRGB(100, 0, 255)
            end
            TweenService:Create(espBtn, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
        end)
    end,
    ["Aimbot"] = function(cont)
        local aimbotFrame = Instance.new("Frame", cont)
        aimbotFrame.Size = UDim2.new(1, 0, 0, 30)
        aimbotFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

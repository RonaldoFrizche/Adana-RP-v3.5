        tpBtn.Text = "Sonraki"
        tpBtn.TextColor3 = Color3.new(1, 1, 1)
        tpBtn.TextScaled = true
        Instance.new("UICorner", tpBtn).CornerRadius = UDim.new(0, 5)
        tpBtn.MouseButton1Click:Connect(function()
            currentTpIndex = (currentTpIndex % #tpLocations) + 1
            teleportTo(currentTpIndex)
            TweenService:Create(tpBtn, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
        end)

        local gunFrame = Instance.new("Frame", cont)
        gunFrame.Size = UDim2.new(1, 0, 0, 30)
        gunFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        Instance.new("UICorner", gunFrame).CornerRadius = UDim.new(0, 5)

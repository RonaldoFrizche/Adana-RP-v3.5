        gunLabel.BackgroundTransparency = 1
        gunLabel.TextColor3 = Color3.new(1, 1, 1)
        gunLabel.TextScaled = true
        gunLabel.TextWrapped = true
        gunLabel.TextXAlignment = Enum.TextXAlignment.Left
        gunLabel.Text = "Silahçı Işınlan"
        Instance.new("UIPadding", gunLabel).PaddingLeft = UDim.new(0, 8)
        local gunBtn = Instance.new("TextButton", gunFrame)
        gunBtn.Position = UDim2.new(0.75, 0, 0.1, 0)
        gunBtn.Size = UDim2.new(0.2, 0, 0.8, 0)
        gunBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 0)
        gunBtn.Text = "Işınlan"
        gunBtn.TextColor3 = Color3.new(1, 1, 1)
        gunBtn.TextScaled = true
        Instance.new("UICorner", gunBtn).CornerRadius = UDim.new(0, 5)
        gunBtn.MouseButton1Click:Connect(function()
            teleportToGunDealer()
            TweenService:Create(gunBtn, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
        end)
    end,
    ["ESP"] = function(cont)
        local espFrame = Instance.new("Frame", cont)
        espFrame.Size = UDim2.new(1, 0, 0, 30)
        espFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        Instance.new("UICorner", espFrame).CornerRadius = UDim.new(0, 5)

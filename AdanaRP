            TweenService:Create(aimbotBtn, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
        end)
    end,
    ["Silent Aim"] = function(cont)
        local silentFrame = Instance.new("Frame", cont)
        silentFrame.Size = UDim2.new(1, 0, 0, 30)
        silentFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

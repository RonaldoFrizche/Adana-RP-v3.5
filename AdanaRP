            silentBtn.BackgroundColor3 = isSilentAimOn and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 100)
            TweenService:Create(silentBtn, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
        end)
    end
}

local catNames = {"Anasayfa", "Main", "Farm", "ESP", "Aimbot", "Silent Aim"}
local catButtons = {}
local selected = "Anasayfa"

for _, cat in ipairs(catNames) do
    local catBtn = Instance.new("TextButton", cb)
    catBtn.Size = UDim2.new(1, -16, 0, 30)
    catBtn.Position = UDim2.new(0, 8, 0, 0)
    catBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    catBtn.Text = cat
    catBtn.TextColor3 = Color3.new(1, 1, 1)
    catBtn.TextScaled = true
    catBtn.Font = Enum.Font.SourceSansSemibold
    Instance.new("UICorner", catBtn).CornerRadius = UDim.new(0, 5)
    catButtons[cat] = catBtn
    catBtn.MouseButton1Click:Connect(function()
        selected = cat
        for c, b in pairs(catButtons) do
            b.BackgroundColor3 = c == selected and Color3.fromRGB(50, 50, 50) or Color3.fromRGB(30, 30, 30)
        end
        for _, child in ipairs(c:GetChildren()) do
            if child:IsA("Frame") or child:IsA("TextLabel") then
                child:Destroy()
            end
        end
        categories[cat](c)
    end)
end

catButtons["Anasayfa"].BackgroundColor3 = Color3.fromRGB(50, 50, 50)
categories["Anasayfa"](c)

-- Sürükleme Fonksiyonu
local function makeDraggable(handle, frm)
    local dragging
    local dragInput
    local dragStart
    local startPos
    local function update(input)
        local delta = input.Position - dragStart
        frm.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    handle.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = frm.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    handle.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end)
end

makeDraggable(t, m)

-- Minimize Fonksiyonu
local mini
mb.MouseButton1Click:Connect(function()
    m.Visible = false
    if not mini then
        mini = Instance.new("TextButton", g)
        mini.Size = UDim2.new(0, 40, 0, 40)
        mini.Position = UDim2.new(1, -50, 0.5, -20)
        mini.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
        mini.Text = ""
        Instance.new("UICorner", mini).CornerRadius = UDim.new(0, 20)
        local miniStroke = Instance.new("UIStroke", mini)
        miniStroke.Color = Color3.fromRGB(255, 255, 255)
        miniStroke.Transparency = 0.8
        miniStroke.Thickness = 2
        mini.MouseButton1Click:Connect(function()
            m.Visible = true
            mini.Visible = false
        end)
        makeDraggable(mini, mini)
    end
    mini.Visible = true
end)

-- T tuşu ile Işınlanma
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.T then
        currentTpIndex = (currentTpIndex % #tpLocations) + 1
        teleportTo(currentTpIndex)
    end
end)

-- Güncelleme Döngüsü
RunService.Heartbeat:Connect(function(dt)
    updateFlying()
    updateAimbot()
    updateSpeed()
end)
